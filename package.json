{
  "name": "app",
  "main": "./main.js",
  "scripts": {
    "dev:main": "webpack --config ./webpack.main.config.js --mode development && electron ./dist/main.js",
    "dev:renderer": "webpack-dev-server --config ./webpack.renderer.config.js --mode development",
    "prod:main": "webpack --config ./webpack.main.config.js --mode production",
    "prod:renderer": "webpack --config ./webpack.renderer.config.js --mode production",
    "package": "electron-packager ./dist --platform win32 --arch x64 --out ./release --overwrite",
    "release": "npm run prod:main && npm run prod:renderer && npm run package"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
